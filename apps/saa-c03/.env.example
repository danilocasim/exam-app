# Mobile App Environment Variables
# Copy this file to .env and fill in the values

# Backend API Base URL
EXPO_PUBLIC_API_URL=https://exam.railway.app

# Exam Type ID - identifies which certification exam this app supports
EXPO_PUBLIC_EXAM_TYPE_ID=SAA-C03

# Google OAuth Web Client ID (from Google Cloud Console)
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your-google-web-client-id.apps.googleusercontent.com

# Optional: Google OAuth Android Client ID (for standalone Android builds only)
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=your-android-client-id.apps.googleusercontent.com
